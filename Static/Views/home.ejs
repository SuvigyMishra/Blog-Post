<%- include('./Partials/header'); -%> 

<% if (posts.length != 0) {%>
<ul type="none">
    <% for (let i of posts) {%>
    <li>
        <h2 class="title"><%= i.title %></h2>
        <p class="createdAt"><%= i.createdAt.toDateString() %></p>
        <p class="content"><%= i.content %></p>
    </li>
    <% } %>
</ul>
<%} else {%> 
    <h2>Ooops.. Nothing to see here!</h2>
<%} %> 
<%- include('./Partials/footer'); -%>

